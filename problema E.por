programa {
  funcao inicio() {
    //declaracao de variaveis
    real fahrenheit,celsius

    //entrada de dados
    escreva("informe a temperadura em fahrenheit ")
    leia(fahrenheit)
    
    //processamento
    celsius=((fahrenheit-32)*5)/9

    //saida de dados
    escreva("essa temperatura em celsius e ",celsius)
  }
}
