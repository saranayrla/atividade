programa {
  funcao inicio() {
    //declaracao de variaveis
    real volume, raio, altura

    //declaracao de constante
    const real pi=3.14159

    //entrada de dados
    escreva("informe o raio ")
    leia(raio)
    escreva("informe a altura ")
    leia(altura)

    //processamento
    volume=pi*raio*altura

    //saida de dados
    escreva("a capacidade da lata e de ",volume)

  }
}
