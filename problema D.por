programa {
  funcao inicio() {
    //declaracao de variaveis
    real fahrenheit,celsius

    //entrada de dados
    escreva("informe a temperadura em celsius ")
    leia(celsius)
    
    //processamento
    fahrenheit=(9*celsius+160)/9

    //saida de dados
    escreva("essa temperatura em fahrenheit e ",fahrenheit)
  }
}
